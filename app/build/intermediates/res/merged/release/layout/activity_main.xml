                        android:drawableTop="@mipmap/ye_ji"
                        android:drawablePadding="5dp"
                        android:gravity="center"
                        android:tag="binding_4"       
                        android:textColor="@android:color/white"
                        android:textSize="24sp" />
                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
